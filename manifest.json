{
  "manifest_version": 2,
  "name": "Cambridge Here",
  "version": "1.0",
  "description": "Show Cambridge Dictionary word definition right here.",
  "icons": {
    "48": "icons/cam-48.png"
  },
  "permissions": ["activeTab", "contextMenus"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
